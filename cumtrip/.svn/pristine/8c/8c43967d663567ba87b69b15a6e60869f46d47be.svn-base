package cumtrip.vo;

public class S_comentVO {
	
	private String sco_no       ;
	private String sco_content  ;
	private String sco_date     ;
	private String sre_no       ;
	private String mem_email    ;
	
	public String getSco_no() {
		return sco_no;
	}
	public void setSco_no(String sco_no) {
		this.sco_no = sco_no;
	}
	public String getSco_content() {
		return sco_content;
	}
	public void setSco_content(String sco_content) {
		this.sco_content = sco_content;
	}
	public String getSco_date() {
		return sco_date;
	}
	public void setSco_date(String sco_date) {
		this.sco_date = sco_date;
	}
	public String getSre_no() {
		return sre_no;
	}
	public void setSre_no(String sre_no) {
		this.sre_no = sre_no;
	}
	public String getMem_email() {
		return mem_email;
	}
	public void setMem_email(String mem_email) {
		this.mem_email = mem_email;
	}

	
}
