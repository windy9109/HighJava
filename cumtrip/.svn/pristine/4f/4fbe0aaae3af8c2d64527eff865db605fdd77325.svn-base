 <sqlMap  namespace="middle">
	<insert	id='insertspace' parameterClass='middlevo'>
	insert into middle (mid_no, mid_name,mid_location, mid_x,mid_y, mid_ex, mid_cost,
	 mid_web, mid_time, main_code, mid_visit)
