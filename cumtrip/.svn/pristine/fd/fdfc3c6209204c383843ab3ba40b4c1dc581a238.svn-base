body{
	min-width: 1200px;
	text-align: left;
}

#plz{
	display: block;
	flex: justify-content;
	margin-bottom: 10px;
}

*, ::after, ::before {
    box-sizing: content-box;
}

*{
	text-align: left !important;
}

#wrap {
    margin-bottom: 100px !important;
