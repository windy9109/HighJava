.carousel-inner {
	float: right;
	display: inline-block;
	margin: 0 auto;
	margin-bottom: 10px;
	width: 75%;
}

.carousel-inner span {
	width: 30%;
	padding: 10px;
}

.carousel-indicators{
	width: 150px;
