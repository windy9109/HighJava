@charset "UTF-8";

.carousel-inner img {
	width: 100%;
	height: 100%;
	margin-top: 50px;
	margin-bottom: 10px;
}

.carousel slide {
	width: 800px;
	height: 700px;
}

.carousel-inner span {
	width: 22%;
	height: 300px;
	display: inline-block;
}

h4, h5{
	font-weight: bold;
}

#mycarousel2{
	margin-bottom: 100px;
}

#l1 {
