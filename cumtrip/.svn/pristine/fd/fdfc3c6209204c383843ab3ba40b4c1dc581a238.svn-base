	margin-top: 60px;
	margin-bottom: 230px;
}

#l2 {
	float: left;
	display: inline-block;
	width: 25%;
	margin-top: 170px;
	margin-bottom: 240px;
}

#l3 {
	float: left;
	display: inline-block;
